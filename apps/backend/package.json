{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
		"start": "node dist/index.js",
		"dev": "nodemon"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@google-cloud/storage": "^7.7.0",
		"body-parser": "^1.20.2",
		"cors": "^2.8.5",
		"dotenv": "^16.4.2",
		"express": "^4.18.2",
		"mongoose": "^8.1.1",
		"multer": "^1.4.5-lts.1",
		"nodemon": "^3.0.3",
		"qs": "^6.11.2",
		"tsconfig-paths": "^4.2.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/multer": "^1.4.11",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.8",
		"typescript": "^5.3.3"
	},
	"nodemonConfig": {
		"ignore": [
			"**/*.test.ts",
			"**/*.spec.ts",
			".git",
			"node_modules"
		],
		"watch": [
			"src"
		],
		"exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
		"ext": "ts, js"
	}
}
